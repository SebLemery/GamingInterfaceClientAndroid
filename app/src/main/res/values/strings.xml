    <string name="help_menu_icon_desc">Tapping this icon will explain whats on your devices screen and is available through most of the application</string>
    <string name="help_address_title">Server Address</string>
    <string name="help_address_desc">Type the IP Address of the computer running the server.  This can be found  in Windows 10 by going into Settings, then Network and Internet, and usually starts with \"192\"</string>
    <string name="help_port_title">Network Port</string>
    <string name="help_port_desc">This has to match on the server end - by default its 8091</string>
    <string name="help_password_title">Password</string>
    <string name="help_password_desc">This has to match on the server as well, and is used to provide some security</string>
    <string name="help_spinner_title">Screen List</string>
    <string name="help_spinner_desc">This will let you select different screens you have created to use</string>
    <string name="help_screenmanager_title">Screen Manager</string>
    <string name="help_screenmanager_desc">Tapping on this will open up the Screen Manager where you can create, edit, delete, and import/export other screens</string>
    <string name="help_start_title">Start</string>
    <string name="help_start_desc">Tapping this will connect to the server and let you start with the screen you\'ve selected</string>
    <string name="help_addNew_desc">Tap here to add a another screen to the app</string>
    <string name="help_addNew_title">Add New</string>
    <string name="help_importScreen_title">Import Screen</string>
    <string name="help_importScreen_desc">Import an existing screen that you or someone else created</string>
    <string name="help_nameEditor_title">Name Editor</string>
    <string name="help_nameEditor_desc">Edit the name of the screen here - don\'t forget to tap Update to save!</string>
    <string name="help_update_title">Update Name</string>
    <string name="help_update_desc">After typing in a new name, tap this to Save your changes</string>
    <string name="help_edit_title">Screen Editor</string>
    <string name="help_edit_desc">Tap here to Customzie your Screen!</string>
    <string name="help_export_title">Export</string>
    <string name="help_export_desc">This will export your screen and allow you to share with others.  The exported file is called \'GIC-(name of screen).ZIP\' and is saved on your device inside the Downloads folder</string>
    <string name="help_delete_title">Delete Screen</string>
    <string name="help_delete_desc">Tap this to delete the current screen - this cannot be undone!!!</string>
